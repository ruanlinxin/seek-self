{
  "name": "web",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev:panel": "vite --host --open --mode development",
    "dev:mock": "vite --open --mode mock",
    "dev": "vite --host --open --mode development",
    "dev:linux": "vite --mode development",
    "build:test": "vite build --mode test",
    "build": "vite build --mode prd",
    "build:type": "vue-tsc --noEmit",
    "build:site": "vue-tsc --noEmit && vite build --mode site",
    "preview": "vite preview",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0",
    "lint:fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --max-warnings 0 --fix",
    "stylelint": "stylelint src/**/*.{html,vue,sass,less}",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,css,sass,less}",
    "site:preview": "npm run build && cp -r dist _site",
    "test": "echo \"no test specified,work in process\"",
    "test:coverage": "echo \"no test:coverage specified,work in process\""
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "echarts": "6.0.0",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "monaco-editor": "^0.52.2",
    "nanoid": "^5.1.5",
    "nprogress": "^0.2.0",
    "peerjs": "^1.5.5",
    "pinia": "^3.0.3",
    "qs": "^6.14.0",
    "screenfull": "^6.0.2",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vue": "^3.5.18",
    "vue-router": "~4.5.1",
    "vue3-sfc-loader": "^0.9.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/echarts": "^5.0.0",
    "@types/lodash": "^4.17.18",
    "@types/node": "^24.1.0",
    "@types/qs": "^6.14.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue/compiler-sfc": "^3.5.18",
    "code-inspector-plugin": "^1.0.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^10.2.0",
    "eslint-plugin-vue-scoped-css": "^2.10.0",
    "less": "^4.4.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.0",
    "stylelint": "^16.23.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-less": "^3.0.1",
    "stylelint-order": "^7.0.0",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "vite": "^7.0.6",
    "vite-svg-loader": "^5.1.0",
    "vue-eslint-parser": "^10.0.0",
    "vue-tsc": "^3.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "npm run lint:fix",
      "git add ."
    ],
    "*.{html,vue,vss,sass,less}": [
      "npm run stylelint:fix",
      "git add ."
    ]
  }
}
